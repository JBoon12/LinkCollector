  pod 'RealmSwift'
    
  # Async
  pod 'RxSwift', '~> 5'
   pod 'RxCocoa', '~> 5'

  #URLEmbeddedView
  pod 'Kingfisher', '~> 5.0'
  pod 'RxKingfisher'
  pod 'URLEmbeddedView'
  pod 'MisterFusion'

end
