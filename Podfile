platform :ios, '11.0'
inhibit_all_warnings!

target 'LinkCollector' do
  use_frameworks!
  # Networking
  pod 'Alamofire'
  pod 'Kanna', '~> 5.2.2'
  
  # Database
  pod 'RealmSwift'
    
  # Async
  pod 'RxSwift', '~> 5'
  pod 'RxCocoa', '~> 5'
  pod 'Kingfisher', '~> 5.0'
  pod 'RxKingfisher'
  
  # autolayout
  pod 'SnapKit'

end
